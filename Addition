from sys import argv

rest_site = 'AG'

def prt(m):
    if len(m) == 0 or m[1][-3:-1] != rest_site:
        return
    print(m[0][:-1])
    print(m[1][:-1] + 'CT')
    if m[2][0] == '+':
        print('+')
        print(m[3][:-1] + m[3][-3:-1])
    else:
        print(m[2][:-1] + m[2][-3:-1])

f = open(argv[1], 'r')
m = []

for line in f:
    if line[0] == '@':
        if len(m) > 0:
            prt(m)
        m = []       
    m.append(line)
prt(m)

f.close()
